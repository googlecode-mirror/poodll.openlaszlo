	 <question template="TSTART" qname="@name@" entryanim="in"  qback="previous">
            <prompt prompttext=" " width="100" y="0" fsize="2" bcolor="0x000000" fcolor="0xFFFFFF" promptaudio="audio/keisanquiz/@name@.mp3" />
            <button startx="35" name="abutton" starty="20" framecount="0" >
                  <frame  ftext="Begin" fsize="3" width="40" height="24" bstate="0" src="keisanquiz/resources/woodenbanner.png" fcolor="0x00FF00" type="button" >
						<action type="tempproperty" args="ZAscore:0"  />
						<action type="next"  />
						<action type="startbigtimer" args="@milliseconds@" />
						
				  </frame>
            </button>
      </question>
	  <question template="TSUCCESS" qname="@name@" entryanim="in" slot="@slot@" qnext="@next@" qback="previous" timer="3000" timerarg="scoreboard">
            <prompt prompttext=" " width="100" y="0" fsize="2" bcolor="0x000000" fcolor="0xFFFFFF" promptaudio="audio/hooray.mp3" />
            <static startx="20" name="abutton" starty="20" framecount="0" >
                  <frame  width="60" height="60" bstate="0" src="keisanquiz/resources/hooray.gif" type="image" >
						<action type="resetbigtimer" />
						<action type="next"  />
				  </frame>
            </static>
      </question>
	  <question template="TFAIL" qname="@name@" entryanim="in" qback="@back@" timer="4000" timerarg="back">
            <prompt prompttext="@prompt@" width="100" y="0" fsize="2" bcolor="0x000000" fcolor="0xFFFFFF" promptaudio="null" />
            <button startx="20" name="abutton" starty="20" framecount="0" >
                  <frame  width="60" height="60" bstate="0" src="keisanquiz/resources/ohno.gif" type="image" >
						<action type="back"  />
				  </frame>
            </button>
      </question>
	  <question template="TANIMATE" qname="@name@" entryanim="in" qback="start" timer="10000" timerarg="previous">
            <prompt prompttext="@prompt@" width="100" y="0" fsize="2" bcolor="0x000000" fcolor="0xFFFFFF" promptaudio="audio/keisanquiz/@name@.mp3" />
            <button name="abutton" endx="5" endy="10" animtime="400" startx="-50" starty="-50" framecount="0" >
                  <frame  width="60" height="60" bstate="0" src="@a@" src2="@b@" picspeed="3"  type="image" >
						<action type="previous"  />
				  </frame>
            </button>
      </question>
