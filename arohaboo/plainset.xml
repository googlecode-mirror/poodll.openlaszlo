            </button>
            <button systemproperty="XAplayaudioprompt" framecount="1" starty="45" cantoggle="yes" name="audiopromptbutton" startx="20" clicksound="button-9.wav">
                  <frame args="XAplayaudioprompt:no" bstate="0" src="plainset/borderred.png" type="button" action="systemproperty" ftext="Yes Play Prompt" width="40" fcolor="0x0000FF" fsize="18" height="20" />
                  <frame args="XAplayaudioprompt:yes" bstate="1" src="plainset/borderred.png" type="button" action="systemproperty" ftext="No Mute Prompt" width="40" fcolor="0x0000FF" fsize="18" height="20" />
            </button>
            <button systemproperty="XAshowtextprompt" framecount="1" starty="45" cantoggle="yes" name="showpromptbutton" startx="65" clicksound="button-9.wav">
                  <frame args="XAshowtextprompt:no" bstate="0" src="plainset/borderred.png" type="button" action="systemproperty" ftext="Yes Show Prompt" width="40" fcolor="0x0000FF" fsize="18" height="20" />
                  <frame args="XAshowtextprompt:yes" bstate="1" src="plainset/borderred.png" type="button" action="systemproperty" ftext="No Hide Prompt" width="40" fcolor="0x0000FF" fsize="18" height="20" />
            </button>
            <button framecount="0" starty="70" name="returnbutton" startx="40" clicksound="button-9.wav">
                  <frame bstate="0" src="plainset/borderred.png" type="button" action="previous" ftext="Back to Start" width="40" fcolor="0x0000FF" fsize="18" height="20" />
            </button>
      </question>
      <question qname="loader" timer="300" timerarg="next" entryanim="none" qnext="StartRound1" hidetimer="true" bgimage="plainset/blacksquare.png">
            <prompt bcolor="0x000000" width="100" fsize="2" prompttext=" " fcolor="0xFFFFFF" y="0" />
            <static framecount="0" starty="20" name="yesbutton" startx="20" clicksound="button-9.wav">
                  <frame ftext="Loading" bstate="0" action="next" type="text" fcolor="0x00FF00" fsize="4" />
            </static>
            <static framecount="0" starty="60" name="continuebutton" startx="20" clicksound="button-9.wav">
                  <frame bstate="0" src="plainset/loading_y.png" type="image" picspeed="1" width="25" src2="plainset/loading_w.png" action="next" height="10" />
            </static>
      </question>
      <question qname="fail1" entryanim="in" qback="start">
            <prompt bcolor="0x000000" prompttext="You failed .... " promptaudio="plainset/youfailed.mp3" width="100" fcolor="0xFFFFFF" fsize="2" y="0" />
            <button framecount="0" starty="40" name="gobutton" startx="40" clicksound="button-9.wav">
                  <frame ftext="try Round 1 again" bstate="0" action="next" type="text" fcolor="0xFF0000" fsize="9" />
            </button>
            <button framecount="0" starty="70" name="backtostartbutton" startx="40" clicksound="button-9.wav">
                  <frame ftext="or .. back to start" bstate="0" action="back" type="text" fcolor="0xDDDD00" fsize="3" />
            </button>
            <button framecount="0" starty="60" name="showhint" startx="50" clicksound="button-9.wav">
                  <frame ftext="Why?" bstate="0" action="showhint" type="text" fcolor="0xFF0000" fsize="9" />
            </button>
      </question>
      <question preload="EndRound1,fail1" qback="fail1" hinttext="Don't do that again" topbgimage="plainset/backcanyon.jpg" qname="StartRound1" bgimage="plainset/backcanyon.jpg">
            <prompt bcolor="0x000000" prompttext="Which is the smallest of these countries?" promptaudio="plainset/smallestcountry.mp3" width="100" fcolor="0xFFFFFF" fsize="2" y="0" />
            <button framecount="0" starty="25" name="jpbutton" startx="25" clicksound="button-9.wav">
