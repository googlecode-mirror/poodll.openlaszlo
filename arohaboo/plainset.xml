            <button framecount="0" starty="75" name="nzbutton" startx="25" clicksound="button-9.wav">
                  <frame ftext="New Zealand" bstate="0" action="back" type="text" fcolor="0xFF0000" fsize="4" />
            </button>
            <button framecount="0" starty="25" name="itbutton" startx="75" clicksound="button-9.wav">
                  <frame ftext="Italy" bstate="0" action="back" type="text" fcolor="0x00FF00" fsize="4" />
            </button>
            <button framecount="0" starty="65" name="gbbutton" startx="65" clicksound="button-9.wav">
                  <frame ftext="Great Britain" bstate="0" fsize="4" type="text" fcolor="0xFF0000">
                        <action type="next" />
                  </frame>
            </button>
      </question>
      <question qname="banana" qback="fail1" hinttext="Don't do that again">
            <prompt bcolor="0x000000" prompttext="Are you a banana?" promptaudio="plainset/abanana.mp3" width="100" fcolor="0xFFFFFF" fsize="2" y="0" />
            <button framecount="0" animtime="500" starty="20" endy="20" endx="20" name="yesbutton" startx="-20" clicksound="button-9.wav">
                  <frame ftext="Yes" bstate="0" action="back" type="text" fcolor="0x00FF00" fsize="4" />
            </button>
            <button framecount="0" animtime="500" starty="50" endy="50" endx="60" name="nobutton" startx="120" clicksound="button-9.wav">
                  <frame ftext="No" bstate="0" fsize="9" type="text" fcolor="0xFF0000">
                        <action type="next" />
                  </frame>
            </button>
      </question>
     
      <question qback="start" entryanim="up" qname="EndRound1">
            <prompt bcolor="0x000000" prompttext="Round 1 Complete" promptaudio="plainset/youfinished.mp3" width="100" fcolor="0xFFFFFF" fsize="2" y="0" />
            <button framecount="0" starty="20" name="gobutton" startx="40" clicksound="button-9.wav">
                  <frame args="StartRound2" bstate="0" action="loader" type="text" ftext="next round" fcolor="0xFF0000" fsize="9" />
            </button>
      </question>
      <question qname="fail2" entryanim="in" qback="start">
            <prompt bcolor="0x000000" prompttext="You failed .... " promptaudio="plainset/youfailed.mp3" width="100" fcolor="0xFFFFFF" fsize="2" y="0" />
            <button framecount="0" starty="40" name="gobutton" startx="20" clicksound="button-9.wav">
                  <frame ftext="Try Round 2 again ..." bstate="0" action="next" type="text" fcolor="0xFF0000" fsize="7" />
