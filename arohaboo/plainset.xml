            </button>
            <button framecount="1" starty="44" name="angrycatred" startx="40" clicksound="button-9.wav">
                  <frame args="3" bstate="0" src="plainset/angrycatredeyes.png" type="image" action="seqvalidatenext" />
            </button>
            <button framecount="1" starty="10" name="angrycatgreen" startx="15" clicksound="button-9.wav">
                  <frame args="2" bstate="0" src="plainset/angrycatgreeneyes.png" type="image" action="seqvalidate" />
