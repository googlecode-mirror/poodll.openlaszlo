                  <frame bstate="0" src="plainset/bordergreen.png" type="button" action="back" ftext="Options" width="30" fcolor="0xFFFFFF" fsize="3" height="20" />
            </button>
      </question>
      <question qname="options" qback="start">
            <prompt bcolor="0x000000" width="100" fsize="2" prompttext="Tough Quiz!" fcolor="0xFFFFFF" y="0" />
            <button systemproperty="XAplaybacksound" framecount="1" starty="10" cantoggle="yes" name="bgaudiobutton" startx="20" clicksound="button-9.wav">
                  <frame args="XAplaybacksound:no" bstate="0" src="plainset/borderred.png" type="button" action="systemproperty" ftext="Yes Bg Sound" width="25" fcolor="0x0000FF" fsize="18" height="20" />
                  <frame args="XAplaybacksound:yes" bstate="1" src="plainset/borderred.png" type="button" action="systemproperty" ftext="No Bg Sound" width="25" fcolor="0x0000FF" fsize="18" height="20" />
            </button>
            <button systemproperty="XAplaysoundfx" framecount="1" starty="10" cantoggle="yes" name="soundfxbutton" startx="65" clicksound="button-9.wav">
